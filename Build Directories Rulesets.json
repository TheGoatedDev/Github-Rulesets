{
	"name": "Block Pushing Build Directories",
	"target": "push",
	"source_type": "Repository",
	"enforcement": "active",
	"conditions": null,
	"rules": [
		{
			"type": "file_path_restriction",
			"parameters": {
				"restricted_file_paths": [
					"build/**/*",
					"dist/**/*",
					"out/**/*",
					"target/**/*",
					"bin/**/*",
					"release/**/*",
					"public/**/*",
					"output/**/*",
					"compiled/**/*"
				]
			}
		}
	],
	"bypass_actors": []
}
